-- tuples
let point = (3.0, 2.0);
case point in {
	(l, r) => l * r
}
point.0 = 12.3;
let a = point.1;

-- records
let record = { name = "bob", teeth_count = 103 };
case record in {
	{ n = teeth_count } => n + 10
}
record.name = "frank";
let b = record.teeth_count;

-- typedef
const Point : type = (f32, f32);
const fn Pair[a : type] : type = (a, a);