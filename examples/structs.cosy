-- tuples
let point = (3.0, 2.0)

-- records
record Vec {
	x : f32,
	y : f32
}

-- initalisation
let vec = Vec { x = 2.0, y = 4.2 }

-- setting members
vec.x = 5.0     -- record
point.0 = 12.3  -- tuple

-- getting members
let y = vec.y    -- record
let b = point.1  -- tuple

-- typedefs
type Point = (i32, i32)
type Empty = ()